#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/release-it@17.10.0_typescript@5.6.3/node_modules/release-it/bin/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/release-it@17.10.0_typescript@5.6.3/node_modules/release-it/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/release-it@17.10.0_typescript@5.6.3/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/release-it@17.10.0_typescript@5.6.3/node_modules/release-it/bin/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/release-it@17.10.0_typescript@5.6.3/node_modules/release-it/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/release-it@17.10.0_typescript@5.6.3/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../release-it/bin/release-it.js" "$@"
else
  exec node  "$basedir/../release-it/bin/release-it.js" "$@"
fi
