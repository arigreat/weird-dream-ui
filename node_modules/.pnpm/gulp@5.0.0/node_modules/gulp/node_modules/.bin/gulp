#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/gulp-cli@3.0.0/node_modules/gulp-cli/bin/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/gulp-cli@3.0.0/node_modules/gulp-cli/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/gulp-cli@3.0.0/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/gulp-cli@3.0.0/node_modules/gulp-cli/bin/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/gulp-cli@3.0.0/node_modules/gulp-cli/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/gulp-cli@3.0.0/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../gulp-cli@3.0.0/node_modules/gulp-cli/bin/gulp.js" "$@"
else
  exec node  "$basedir/../../../../../gulp-cli@3.0.0/node_modules/gulp-cli/bin/gulp.js" "$@"
fi
