#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules/vite/bin/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules/vite/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules/vite/bin/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules/vite/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules:/mnt/c/Users/ariiii/Documents/program/study_project/FrontEnd/vue/vite_component/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@5.4.9_@types+node@22.10.1_less@4.2.0_sass-embedded@1.80.6_sass@1.80.6/node_modules/vite/bin/vite.js" "$@"
fi
